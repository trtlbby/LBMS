<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="525.0" prefWidth="530.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.testdb.StudentListViewController">
   <children>
      <VBox prefHeight="74.0" prefWidth="530.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" graphicTextGap="5.0" prefHeight="75.0" prefWidth="530.0" text="Student List" textAlignment="CENTER">
               <font>
                  <Font name="Arial Black" size="36.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <TableView fx:id="tblStudentList" layoutX="14.0" layoutY="74.0" prefHeight="400.0" prefWidth="500.0">
        <columns>
          <TableColumn fx:id="colID" prefWidth="81.6000394821167" text="ID" />
          <TableColumn fx:id="colName" prefWidth="166.93334197998047" text="Name" />
            <TableColumn fx:id="colPurpose" prefWidth="169.066650390625" text="Purpose" />
            <TableColumn fx:id="colCollege" prefWidth="71.73330688476562" text="College" />
        </columns>
      </TableView>
      <HBox alignment="CENTER_RIGHT" layoutX="14.0" layoutY="474.0" prefHeight="50.0" prefWidth="500.0">
         <children>
            <Button fx:id="btnCompute" mnemonicParsing="false" onAction="#onClickCompute" text="Compute">
               <HBox.margin>
                  <Insets right="340.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="btnAdd" defaultButton="true" layoutX="394.0" layoutY="22.0" mnemonicParsing="false" onAction="#onClickAdd" text="Add" />
            <Button fx:id="btnBack" cancelButton="true" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#onClickBack" text="Back">
               <HBox.margin>
                  <Insets left="2.5" right="5.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
